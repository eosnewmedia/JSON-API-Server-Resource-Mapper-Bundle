{
  "name": "enm/json-api-server-resource-mapper-bundle",
  "description": "Symfony integration for enm/json-api-server-resource-mappers",
  "type": "library",
  "require": {
    "php": ">=7.0",
    "enm/json-api-server-resource-mappers": "^1.1",
    "symfony/dependency-injection": "^2.7|^3.0",
    "symfony/http-kernel": "^2.7|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    }
  ],
  "config": {
    "platform": {
      "php": "7.0"
    }
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Enm\\Bundle\\JsonApi\\Server\\ResourceMappers\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enm\\Bundle\\JsonApi\\Server\\ResourceMappers\\Tests\\": "tests"
    }
  }
}
